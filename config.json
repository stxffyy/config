{
    "repositories": [
         {
            "id": 0,
            "title": "lottie-web",
            "url": "https://github.com/airbnb/lottie-web"
        }
    ],
    "rules": [
        {
            "id": 0,
            "title": "noNewLineAtTheEnd",
            "description": "В файле отсутствует перенос строки",
            "ruleImplementation": "./rules/noNewLineAtTheEnd.js",
            "pattern": "**/*(*.js|*.html|package.json|*.css)"
        },
        {
            "id": 1,
            "title": "checkReactVersion",
            "description": "Версия React ниже 18.0.0",
            "ruleImplementation": "./rules/checkReactVersion.js",
            "pattern": "**/package.json"
        }
    ]
}
